{
  "name": "gram-captcha",
  "version": "1.0.0",
  "description": "a simple bot that checks whether a new chat member is a bot",
  "main": "./build/main.js",
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node ./build/main.js",
    "start:dev": "concurrently \"npm run watch:ts\" \"npm run watch:js\"",
    "watch:ts": "tsc -w",
    "watch:js": "nodemon ./build/main.js"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "concurrently": "^7.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "module-alias": "^2.2.2",
    "nconf": "^0.12.0",
    "telegraf": "^4.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.15",
    "@types/module-alias": "^2.0.1",
    "@types/nconf": "^0.10.2",
    "@types/node": "^17.0.24",
    "nodemon": "^2.0.15",
    "typegram": "^3.8.0",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/desrev3r/gram-captcha.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/desrev3r/gram-captcha/issues"
  },
  "homepage": "https://github.com/desrev3r/gram-captcha#readme",
  "engines": {
    "node": "16.x"
  },
  "_moduleAliases": {
    "@utils": "./build/utils/"
  }
}
